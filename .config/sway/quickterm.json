{
    "menu": "rofi -dmenu -p 'quickterm: ' -no-custom -auto-select",
    "term": "alacritty",
    "history": "{$HOME}/.cache/i3-quickterm.order",
    "ratio": 0.25,
    "pos": "top",
    "shells": {
        "js": "node",
        "python": "ipython3 --no-banner",
        "shell": "{$SHELL}",
        "tmux": "zsh -c 'tmux a -t quickterm || tmux new -s quickterm'"
    }
}
